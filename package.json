{
  "name": "turbo-kit",
  "version": "0.0.1",
  "author": "Justin Bassey",
  "license": "MIT",
  "workspaces": [
    "apps/*",
    "config/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "jest-config": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.6.0"
}
